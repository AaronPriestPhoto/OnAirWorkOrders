# Copy to .env and fill in your values
ONAIR_API_KEY=
ONAIR_COMPANY_ID=
# Optional: If your server differs, set explicit base URL override
# Example: https://server1.onair.company/api/v1
ONAIR_BASE_URL=
# Run mode: online | offline (default is online). Use offline for faster local testing.
ONAIR_RUN_MODE=online
ONAIR_TIMEOUT_SECONDS=30
ONAIR_MAX_RETRIES=3
ONAIR_DB_PATH=onair_jobs.db
# Airport cache TTL (days)
ONAIR_AIRPORT_CACHE_DAYS=90
